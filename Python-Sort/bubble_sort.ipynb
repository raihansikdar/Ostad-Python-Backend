   "source": [
    "py_array = [64, 34, 25, 12, 22, 11, 90, 5]\n",
    "\n",
    "n = len(py_array)\n",
    "\n",
    "print(n)\n",
    "print(n-1)\n",
    "\n",
    "for i in range(n):\n",
    "    for j in range(n-i-1):\n",
    "        if py_array[j] > py_array[j+1]:\n",
    "            py_array[j],py_array[j+1] = py_array[j+1],py_array[j]\n",
    "\n",
    "print(\"Sorted array: \",py_array)"
   ]
  }
